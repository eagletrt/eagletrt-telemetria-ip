[Unit]
Description=Eagle-TRT telemetry ip share
StartLimitIntervalSec=0
[Service]
Type=simple
Restart=always
RestartSec=5
User=ubuntu
ExecStart=/home/ubuntu/eagletrt-telemetria-ip/client/shell/client.sh
WorkingDirectory=/home/ubuntu/eagletrt-telemetria-ip/client/shell
[Install]
WantedBy=multi-user.target